<%-include('./header.ejs')%>

<div class="bd-example">
    <div id="carouselExampleCaptions" class="carousel slide"
        data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleCaptions" data-slide-to="0"
                class="active"></li>
            <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
            <li data-target="#carouselExampleCaptions" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="image/Banner-Couse-1.jpg"
                    class="d-block w-100 banner-image" alt="...">
                <div class="carousel-caption ">
                    <h5>Bachelor of Technology</h5>
                    <p>Unleash your potential with a transformative journey in
                        the dynamic world of B.Tech.</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="image/mtech.jpg"
                    class="d-block w-100 banner-image" alt="...">
                <div class="carousel-caption ">
                    <h5>Master of Technology</h5>
                    <p>Unleash the engineer within and shape the future with our
                        comprehensive MTech curriculum</p>
                </div>
            </div>
            <div class="carousel-item">
                <img src="image/mba.jpg" class="d-block w-100 banner-image"
                    alt="...">
                <div class="carousel-caption ">
                    <h5>Master of Business Administration</h5>
                    <p> Elevate your career aspirations with an MBA that goes
                        beyond business education</p>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleCaptions"
            role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleCaptions"
            role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

<!-- marguee start  -->
<div class="mark">
    <marquee direction scrollamount="7">
        <img src="https://www.rgpv.ac.in/Images/new_icon_blink.gif">
        Admission Portal | CONTACT US: admission@gmail.com
    </marquee>
</div>
<!-- marquee end -->
<!-- container fluid start welcome section -->
<div class="container-fluid bg-secondary">
    <h2 class="text-center fiddle ">Welcome <%= n %>
        <b></b>
    </h2>

    <div class="container " style="padding: 0px 0px 55px 0px;">
        <div class="row">

            <div class="col-md-7">
                <p class="text-justify banner-content">Bachelor of Technology for
                    Tomorrow: Our BTech program is designed to propel you into
                    the forefront of innovation and advanced engineering. Dive
                    into specialized coursework, groundbreaking research
                    projects, and industry collaborations. Gain a competitive
                    edge with hands-on experience and mentorship from industry
                    leaders. Elevate your career potential with an MTech degree
                    that goes beyond boundaries. Your journey to mastery begins
                    here.</p>

                <button class="btn btn-warning shadow form-group ">READ
                    MORE</button>
            </div>
            <div class="col-md-5 ">
                <img class="w-100 web-logo" alt="about"
                    src="image/b-tech.jpg">
            </div>

        </div>
    </div>
</div>
<!-- container fluid end welcome section -->
